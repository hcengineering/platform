    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest":"^29.5.5",
    "typescript":"^5.3.3"
  },
  "#replaces": [
    ".eslintrc.js",
